  padding: 0.8rem 1.5rem;
  width: 100%;
}

/* Toast Notification */
.toast {
  position: fixed;
  bottom: 20px;
  right: 20px;
  background-color: var(--whatsapp-verde);
  color: var(--branco);
  padding: 0.8rem 1.5rem;
  border-radius: 5px;
  z-index: 1000;
  display: none;
  animation: slideIn 0.5s ease, fadeOut 0.5s ease 2.5s;
}

.toast:not([hidden]) {
  display: block;
}

@keyframes slideIn {
  from { transform: translateX(100%); }
  to { transform: translateX(0); }
}

@keyframes fadeOut {
  to { opacity: 0; }
}

